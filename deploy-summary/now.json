{
  "routes": [
    { "src": "/api/callback", "dest": "/api/callback.js" },
    { "src": "/api/connect", "dest": "/api/connect.js" },
    { "src": "/api/delete", "dest": "/api/delete.js" },
    { "src": "/api/setup", "dest": "/api/setup.js" },
    { "src": "/api/ui", "dest": "/api/ui.js" },
    { "src": "/api/webhook", "dest": "/api/webhook.js" }
  ],
  "env": {
    "CLIENT_ID": "@deploy-summary-integration-client-id",
    "CLIENT_SECRET": "@deploy-summary-integration-client-secret",
    "GITHUB_CLIENT_ID": "@deploy-summary-integration-github-client-id",
    "GITHUB_CLIENT_SECRET": "@deploy-summary-integration-github-client-secret",
    "GITLAB_CLIENT_ID": "@deploy-summary-integration-gitlab-client-id",
    "GITLAB_CLIENT_SECRET": "@deploy-summary-integration-gitlab-client-secret",
    "MONGO_URI": "@deploy-summary-integration-mongo-uri",
    "INTEGRATION_HOMEPAGE": "https://zeit.co/integrations/deploy-summary",
    "INTEGRATION_URL": "https://deploy-summary.now.sh",
    "MICROLINK_API_KEY": "@microlink-api-key",
    "SENTRY_DSN": "@deploy-summary-sentry-dsn"
  }
}
